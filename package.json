{
    "name": "rylen",
    "version": "0.0.0",
    "private": true,
    "workspaces": [
        "apps/*",
        "packages/*",
        "packages/integrations/*"
    ],
    "scripts": {
        "build": "turbo run build",
        "dev": "turbo run dev --parallel",
        "lint": "turbo run lint",
        "format": "prettier --write \"**/*.{ts,tsx,md}\""
    },
    "devDependencies": {
        "prettier": "latest",
        "turbo": "latest"
    },
    "packageManager": "pnpm@7.5.1"
}
